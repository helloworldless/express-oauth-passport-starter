{
  "name": "express-oauth-passport-starter",
  "description": "Starter project using Passport for OAuth with Express; Deployable to Google App Engine",
  "version": "1.0.0",
  "license": "MIT",
  "author": "David A. Good",
  "engines": {
    "node": ">=8"
  },
  "repository": "https://github.com/paltamadura/express-oauth-passport-starter",
  "main": "app.js",
  "cloud-repo-tools": {
    "requiresKeyFile": true,
    "requiresProjectId": true,
    "test": {
      "app": {
        "args": [
          "app.js"
        ],
        "msg": "Hello, world!"
      }
    }
  },
  "scripts": {
    "start": "nodemon app.js",
    "e2e": "repo-tools test deploy",
    "test": "repo-tools test app",
    "cover": "nyc --cache npm test; nyc report --reporter=html"
  },
  "dependencies": {
    "body-parser": "^1.19.0",
    "ejs": "^3.0.1",
    "express": "^4.16.4",
    "express-session": "^1.17.0",
    "node-fetch": "^2.6.0",
    "nodemon": "^2.0.2",
    "passport": "^0.4.1",
    "passport-google-oauth2": "^0.2.0",
    "passport-google-oauth20": "^2.0.0",
    "session-file-store": "^1.3.1"
  },
  "devDependencies": {
    "@google-cloud/nodejs-repo-tools": "^3.0.0",
    "ava": "^0.25.0",
    "supertest": "^4.0.0"
  }
}
